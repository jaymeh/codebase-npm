{
  "name": "codebase-npm",
  "version": "1.0.3",
  "description": "NPM Package that interacts with CodebaseHQ",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaymeh/codebase-npm.git"
  },
  "keywords": [
    "CodebaseHQ",
    "Codebase",
    "Time Tracking"
  ],
  "author": "C-J",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaymeh/codebase-npm/issues"
  },
  "homepage": "https://github.com/jaymeh/codebase-npm#readme",
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/node": "^11.10.5",
    "jest": "^24.3.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "collectionsjs": "^0.3.2",
    "decamelize-keys": "^1.1.0",
    "fast-xml-parser": "^3.12.13",
    "js2xmlparser": "^4.0.0",
    "p-iteration": "^1.1.7"
  }
}
